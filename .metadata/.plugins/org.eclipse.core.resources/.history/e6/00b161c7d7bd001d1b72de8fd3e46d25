package practice;

public class stringmethod {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String o="name";
		String a="pawar";
		
		System.out.println(o.charAt(1));
		System.out.println(o.codePointAt(0));
		//System.out.println(o.compareTo(a));
		
		
		
		System.out.println(o.endsWith(a));
		System.out.println(o.equals(a));
		
		System.out.println(o.split(a,0));//this method provide the annotation code
	    System.out.println(o.hashCode());
	
	    System.out.println(o.indent(3)); // provide the space as per no
	
	   System.out.println(a.isBlank());
	   System.out.println(a.isEmpty());
	
	   System.out.println(o.length());
	 
	System.out.println(o.toLowerCase());
	System.out.println(o.toUpperCase());
	
	
	}

}
