
public class work {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
		int i,j,k;
		for (i=1;i<=4;i++) 
		{
			for (j=5;j>i;j--)
			{
				System.out.println(" ");
			}
			for(k=1;k<=i;k++)
			{
			System.out.println("* ");
		}
		System.out.println("\n");
	}

	}
}
