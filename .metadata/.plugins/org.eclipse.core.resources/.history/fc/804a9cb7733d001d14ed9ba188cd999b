package typecasting;

public class demo {
	public void today() {
		byte s = 12;
		int a=s;
		float b = a;
		long h =(long) b;
		double k =  h ;
		
		System.out.println(b);
		System.out.println(a);
		System.out.println(h);
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    demo obj = new demo();
    obj.today();
	}

}
