package typecasting;

public class demo {
	public void today() {
		
		byte a = 40;
		int b = a;
		short c = a;
		float f = c;
		double s =30+c;
		long  j =(long)+(float) s;
		
		System.out.println(j);
		System.out.println(b);
		System.out.println(f);
		System.out.println(s);
		System.out.println(a);
		System.out.println(c);
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    demo obj = new demo();
    obj.today();
	}

}
