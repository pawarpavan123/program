<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login_controller</servlet-name>
  	<display-name>login_controller</display-name>
  	<description></description>
  	<servlet-class>controller.login_controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>studententity_reg</servlet-name>
  	<display-name>studententity_reg</display-name>
  	<description></description>
  	<servlet-class>controller.studententity_reg</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login_controller</servlet-name>
  	<url-pattern>/login_controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>studententity_reg</servlet-name>
  	<url-pattern>/studententity_reg</url-pattern>
  </servlet-mapping>
</web-app>
